    "vuex": "^3.4.0",
    "webpack-bundle-analyzer": "^4.4.2"
  },
  "peerDependencies": {
    "axios": "^0.21.1",
    "lodash": "^4.17.20",
    "qs": "^6.10.3",
    "view-design": "^4.5.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
